#include<stdio.h>
int isprime(int num)
{
	int i;
	if(num==0||num==1)
		return 0;
	for(i=2;i<=num/2;i++)
	{
		if(num%i==0)
		return 0;
	}
	return 1;
}
int main()
{
	int n;
	printf("Enter the number\n");
	scanf("%d",&n);
	if(isprime(n))
		printf("The given number %d is prime\n",n);
	else
		printf("The given number %d is not a prime\n",n);
}
